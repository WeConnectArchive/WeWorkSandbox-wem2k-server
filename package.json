{
  "name": "wem2k-server",
  "version": "0.1.0",
  "description": "A tiny proxy server that can mock the response for some requests",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "debug": "DEBUG='*' npm run start",
    "doc": "typedoc --out dist/docs --mode modules --exclude 'node_modules/**' .",
    "lint": "tslint --project .",
    "lint-ci": "npm run lint -- --format junit --out reports/lint/lint.xml",
    "server": "node dist/index.js",
    "start": "npm run server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "testing"
  ],
  "author": "testinfra@wework.com",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/debug": "^4.1.3",
    "@types/express": "^4.16.1",
    "@types/http-proxy": "^1.17.0",
    "@types/jest": "^24.0.11",
    "@types/mock-require": "^2.0.0",
    "@types/nock": "^9.3.1",
    "@types/request": "^2.48.1",
    "@types/temp": "^0.8.33",
    "express": "^4.16.4",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "temp": "^0.9.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.15.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "config": "^3.0.1",
    "debug": "^4.1.1",
    "http-proxy": "^1.17.0",
    "jwt-simple": "^0.5.6",
    "nock": "^10.0.6",
    "request": "^2.88.0"
  }
}
